		Scanner scan = null;
		FileReader fr = null;
		try {
			fr = new FileReader(".\\src\\com\\ch15\\scan.txt");
			scan = new Scanner(fr);
			while(scan.hasNextDouble()) {
				
			}
			while(scan.hasnext()) {
				
			}
		} catch (Exception e) {
			// TODO: handle exception
		}
	}
}
